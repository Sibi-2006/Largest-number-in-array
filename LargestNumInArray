public class LargestNumInArray {
    public static void main(String[] args) {
        int a[]={54,67,-9,78,0};
        int n= a.length;
        int max=a[0];//To store the min value .
        for(int i=0;i<n;i++){
            if(a[i]>max){
                max=a[i];
            }
        }
        System.out.println("The largest number is : "+max);
    }
}
